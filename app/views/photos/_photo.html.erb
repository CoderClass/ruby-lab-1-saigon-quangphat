<div class="card">
  <img class="card-img-top" src=<%= photo.url %> alt="Card image cap">
  <div class="card-block">
    <h4 class="card-title"><%= photo.username %></h4>
    <p class="card-text"><%= photo.caption %> <br> <%= photo.likes_count%> likes</p>
  </div>
  <div class="card-footer">
  	<%= render photo.comments %>
  </div>
</div>